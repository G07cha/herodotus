{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "isolatedModules": false,
    "outDir": "dist",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true
  },
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": true,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
